{
  "name": "todo-list",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "-----DEV-----": "",
    "start:dev": "npm run build:dev && http-server -p 8100 dist/todo-list",
    "build:dev": "ng build todo-list",
    "postbuild:dev": "npm run build:webpack:dev",
    "build:webpack:dev": "rimraf ./dist/todo-list/service-worker.js && webpack --config ./webpack.dev.config.js --progress && node ./workbox-build-inject.js",
    "-----PROD-----": "",
    "start:prod": "npm run build:prod && http-server -p 8100 dist/todo-list",
    "build:prod": "ng build todo-list --prod",
    "postbuild:prod": "npm run build:webpack:prod",
    "build:webpack:prod": "rimraf ./dist/todo-list/service-worker.js && webpack --config ./webpack.prod.config.js --progress && node ./workbox-build-inject.js",
    "-----LOCAL-----": "",
    "start:local": "npm run build:webpack:local && ng serve",
    "build:webpack:local": "rimraf ./src/service-worker.js && webpack --config ./webpack.local.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@ngrx/store": "^16.2.0",
    "dotenv-webpack": "^8.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "workbox-background-sync": "7.1.0",
    "workbox-broadcast-update": "7.1.0",
    "workbox-cacheable-response": "7.1.0",
    "workbox-core": "7.1.0",
    "workbox-expiration": "7.1.0",
    "workbox-navigation-preload": "7.1.0",
    "workbox-precaching": "7.1.0",
    "workbox-routing": "7.1.0",
    "workbox-strategies": "7.1.0",
    "workbox-window": "7.1.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "autoprefixer": "^10.4.15",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "ts-loader": "8.4.0",
    "typescript": "~5.1.3",
    "workbox-build": "7.1.0"
  }
}
